{
    "name": "open-runtimes/open-runtimes",
    "description": "Open Runtimes PHP project for testing. PHPUnit tests can test all runtimes automatically.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\": "tests"}
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^6.0",
        "phpunit/phpunit": "^9.5.13",
        "ext-curl": "*",
        "utopia-php/websocket": "^0.3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
